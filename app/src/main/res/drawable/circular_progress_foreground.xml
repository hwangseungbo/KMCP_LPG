<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#C8CBCE" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#B2CCFF" />
            </shape>
        </clip>
    </item>
    <item>
        <shape>
            <size
                android:width="2dp"
                android:height="10dp"
                />
            <corners
                android:topRightRadius="250dp"
                android:topLeftRadius="250dp"
                />
            <gradient
                android:startColor="#e5ebef"
                android:centerColor="#8fc9d4"
                android:endColor="#70ccbb"
                android:type="sweep"
                />
        </shape>
    </item>
</layer-list>
